# Function to count even and odd numbers
def count_even_odd(start, end):
    even_count = 0
    odd_count = 0
    
    # Loop through the range from start to end
    for num in range(start, end + 1):
        if num % 2 == 0:
            even_count += 1
        else:
            odd_count += 1
    
    # Print the counts
    print("Even numbers:", even_count)
    print("Odd numbers:", odd_count)

# Calling the function with range 1 to 9
count_even_odd(1, 9)
